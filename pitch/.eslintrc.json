{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true,
        "react-native/react-native": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        "no-use-before-define":"off",
        "react/prop-types":"off",
        "react/jsx-props-no-spreading": "off"
    },
    "settings": {
        "import/resolver": {
          "babel-module":{}
        }
      }
}
